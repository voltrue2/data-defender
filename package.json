{
	"name": "data-defender",
	"description": "A database-agnostic simple data schema enforcer for node.js",
	"version": "1.0.3",
	"author": "Nobuyori Takahashi <voltrue2@yahoo.com>",
	"repository": {
		"type": "git",
		"url": "https://github.com/voltrue2/data-defender.git"
	},
	"dependencies": {
		"node-uuid": "1.4.3"
	},
	"devDependencies": {
		"mocha": "1.20.1"
	},
	"engine": "node >= 1.10.20",
	 "jshintConfig": {
                "node": true,
                "bitwise": false,
                "camelcase": true,
                "curly": true,
                "eqeqeq": true,
                "forin": false,
                "immed": true,
                "latedef": false,
                "newcap": true,
                "noarg": true,
                "noempty": true,
                "undef": true,
                "unused": true,
                "nonew": true,
                "white": true,
                "maxdepth": 5,
                "quotmark": "single",
                "globals": {
                        "mocha": false,
                        "describe": false,
                        "it": false,
                        "before": false,
                        "beforeEach": false,
                        "after": false,
                        "afterEach": false
                }
        },
	"license": "MIT",
	"contributors": [],
	"keywords": [
		"data",
		"schema",
		"database",
		"constraints"
	]
}
